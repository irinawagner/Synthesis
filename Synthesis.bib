@article{baynham1996,
  title = {Direct Speech: {{What}}'s It Doing in Non-Narrative Discourse?},
  volume = {25},
  issn = {03782166},
  doi = {10.1016/0378-2166(94)00074-3},
  abstract = {Traditional syntactic accounts of speech-reporting presuppose an original utterance and a set of decontextualized operations which transform direct into indirect speech. Pragmatic and discourse-pragmatic accounts of speech reporting analyze its functions in situated discourse and emphasize its rhetorical nature. The vast majority of analyses of speech reporting, whether from within linguistics, pragmatics or literary theory presuppose a narrative discourse frame. Certain types of narrative discourse (autobiography and narratives of personal experience) naturalize the notion of the original utterance, since the narrative purports to represent or re-present historical or biographical incident. In contrast, the occurrence of direct speech in non-narrative discourse unmasks or foregrounds the constructed, rhetorical nature of direct speech 'reporting' in these discourse contexts. In this paper, I present speech report data from non-narrative contexts in classroom discourse and examine the implications of this data for our understanding of the phenomenon of speech reporting, arguing for situated accounts of speech reporting which are sensitive to discourse context. Earlier versions of this paper were given at the Lancaster Linguistics Circle and as a poster at the 4th International Pragmatics Conference in Kobe, Japan. I am grateful to an anonymous reviewer, whose insightful comments greatly improved some aspects of the analysis presented.},
  timestamp = {2016-09-27T17:58:41Z},
  number = {1},
  journaltitle = {Journal of Pragmatics},
  author = {Baynham, Mike},
  date = {1996},
  pages = {61--81},
  groups = {Reported Speech}
}

